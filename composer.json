{
    "name": "theofanisv/filament-edit-page-browser-test",
    "description": "Automated browser testing framework for Filament v4/v5 Edit Pages using Pest",
    "keywords": [
        "filament",
        "filament v4",
        "filament v5",
        "pest",
        "testing",
        "browser testing",
        "automated testing",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Theofanis Vardatsikos",
            "email": "vardtheo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "filament/filament": "^4.0|^5.0",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Theograms\\EditPageTester\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theograms\\EditPageTester\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
